[
  {
    "name": "orcid-update",
    "image": "datacite/toccatore",
    "cpu": 256,
    "memory": 1000,
    "essential": true,
    "command": [
      "toccatore", "orcid_update",
      "--push_url", "${var.push_url}",
      "--access_token", "${var.access_token}",
      "--slack_webhook_url", "${var.webhook_url}"
    ]
  }
]
