[
  {
    "name": "sitemaps-generator",
    "image": "datacite/maltese",
    "cpu": 512,
    "memory": 2000,
    "essential": true,
    "environment" : [
      {
        "name" : "SITEMAP_BUCKET",
        "value" : "search.datacite.org"
      },
      {
        "name" : "RACK_ENV",
        "value" : "production"
      },
      {
        "name" : "AWS_ACCESS_KEY_ID",
        "value" : "${access_key}"
      },
      {
        "name" : "AWS_SECRET_ACCESS_KEY",
        "value" : "${secret_key}"
      },
      {
        "name" : "AWS_REGION",
        "value" : "${region}"
      },
      {
        "name" : "REVISION",
        "value" : "11"
      }
    ]
  }
]
