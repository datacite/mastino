<yandex>
  <profiles>
    <default>
      <log_queries>0</log_queries>
      <log_query_threads>0</log_query_threads>
    </default>
  </profiles>
  <users>
    <default>
      <password remove='1' />
      <password_sha256_hex></password_sha256_hex>
    </default>
    <datacite>
      <password>${datacite_clickhouse_password}</password>

      <networks incl="networks" replace="replace">
        <ip>::/0</ip>
      </networks>
    </datacite>
  </users>
</yandex>
