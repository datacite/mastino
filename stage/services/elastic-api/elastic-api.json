[
  {
    "name": "elastic-api-stage",
    "image": "datacite/levriero",
    "cpu": 1440,
    "memory": 4000,
    "essential": true,
    "hostname": "elastic-api-stage",
    "portMappings": [
      {
        "containerPort": 80
      }
    ],
    "logConfiguration": { 
      "logDriver": "awslogs",
      "options": { 
         "awslogs-group" : "/ecs/elastic-api-stage",
         "awslogs-region": "eu-west-1",
         "awslogs-stream-prefix": "ecs"
      }
    },
    "environment" : [
      {
        "name" : "PASSENGER_APP_ENV",
        "value" : "stage"
      },
      {
        "name" : "RAILS_ENV",
        "value" : "stage"
      },
      {
        "name" : "SITE_TITLE",
        "value" : "DataCite's ElasticSearch supported API"
      },
      {
        "name" : "ELASTIC_PASSWORD",
        "value" : "changeme"
      },
      {
        "name" : "LOG_LEVEL",
        "value" : "info"
      },
      {
        "name" : "JWT_PUBLIC_KEY",
        "value" : "${jwt_public_key}"
      },
      {
        "name" : "JWT_PRIVATE_KEY",
        "value" : "${jwt_private_key}"
      },
      {
        "name" : "SECRET_KEY_BASE",
        "value" : "${secret_key_base}"
      },
      {
        "name" : "CONCURRENCY",
        "value" : "25"
      },
      {
        "name" : "BUGSNAG_KEY",
        "value" : "54fd64d5f37fe13b56fc563f29855aaf"
      },
      {
        "name" : "MEMCACHE_SERVERS",
        "value" : "${memcache_servers}"
      },
      {
        "name" : "LIBRATO_USER",
        "value" : "${librato_email}"
      },
      {
        "name" : "LIBRATO_TOKEN",
        "value" : "${librato_token}"
      },
      {
        "name" : "LIBRATO_SUITES",
        "value" : "${librato_suites}"
      },
      {
        "name" : "APP_URL",
        "value" : "${app_url}"
      },
      {
        "name" : "CDN_URL",
        "value" : "https://assets.test.datacite.org"
      },
      {
        "name" : "VOLPINO_URL",
        "value" : "${volpino_url}"
      },
      {
        "name" : "RE3DATA_URL",
        "value" : "http://www.re3data.org/api/beta"
      },
      {
        "name" : "ES_HOST",
        "value" : "${es_host}"
      },
      {
        "name" : "ES_NAME",
        "value" : "es"
      },
      {
        "name" : "AWS_ACCESS_KEY_ID",
        "value" : "${aws_access_key}"
      },
            {
        "name" : "AWS_SECRET_ACCESS_KEY",
        "value" : "${aws_secret_key}"
      },
      {
        "name" : "AWS_REGION",
        "value" : "${aws_region}"
      },
      {
        "name" : "GITHUB_VERSION",
        "value" : "${version}"
      }
    ]
  }
]
