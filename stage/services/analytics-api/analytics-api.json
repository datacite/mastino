[
    {
        "name": "analytics-api-stage",
        "image": "ghcr.io/datacite/keeshond",
        "cpu": 256,
        "memory": 512,
        "essential": true,
        "networkMode": "awsvpc",
        "portMappings": [
            {
                "containerPort": 80,
                "hostPort": 80
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/analytics-api-stage",
                "awslogs-region": "eu-west-1",
                "awslogs-stream-prefix": "ecs"
            }
        },
        "environment": [
            {
                "name": "HTTP_ADDR",
                "value": ":80"
            },
            {
                "name": "PLAUSIBLE_URL",
                "value": "https://analytics.stage.datacite.org/"
            },
            {
                "name": "DATACITE_API_URL",
                "value": "https://api.stage.datacite.org"
            },
            {
                "name": "GITHUB_VERSION",
                "value": "${version}"
            }
        ]
    }
]