[
  {
    "name": "search-test",
    "image": "crosscite/doi-metadata-search",
    "cpu": 384,
    "memory": 500,
    "essential": true,
    "hostname": "search-test",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 40193
      }
    ],
    "environment" : [
      {
        "name" : "RACK_ENV",
        "value" : "stage"
      },
      {
        "name" : "PASSENGER_APP_ENV",
        "value" : "stage"
      },
      {
        "name" : "SITENAME",
        "value" : "Search Test"
      },
      {
        "name" : "SITE_TITLE",
        "value" : "DataCite Test Search"
      },
      {
        "name" : "JWT_PUBLIC_KEY",
        "value" : "${jwt_public_key}"
      },
      {
        "name" : "JWT_SECRET_KEY",
        "value" : "${jwt_secret_key}"
      },
      {
        "name" : "JWT_HOST",
        "value" : "https://profiles.test.datacite.org"
      },
      {
        "name" : "SECRET_KEY_BASE",
        "value" : "${secret_key_base}"
      },
      {
        "name" : "DATA_URL",
        "value" : "https://data.test.datacite.org"
      },
      {
        "name" : "API_URL",
        "value" : "https://api.test.datacite.org"
      },
      {
        "name" : "ORCID_URL",
