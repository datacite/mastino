      },
      {
        "name" : "BUGSNAG_JS_KEY",
        "value" : "6c736ef992f5685ab781eda05f5b5218"
      },
      {
        "name" : "CDN_URL",
        "value" : "https://assets.test.datacite.org"
      },
      {
