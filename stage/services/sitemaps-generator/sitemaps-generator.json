[
  {
    "name": "sitemaps-generator-stage",
    "image": "datacite/maltese",
    "cpu": 256,
    "memory": 1024,
    "networkMode": "awsvpc",
    "essential": true,
    "logConfiguration": { 
      "logDriver": "awslogs",
      "options": { 
         "awslogs-group" : "/ecs/sitemaps-generator-stage",
         "awslogs-region": "eu-west-1",
         "awslogs-stream-prefix": "ecs"
      }
    },
    "environment" : [
      {
        "name" : "SITEMAP_BUCKET",
        "value" : "search.test.datacite.org"
      },
      {
        "name" : "RACK_ENV",
        "value" : "stage"
      },
      {
        "name" : "AWS_ACCESS_KEY_ID",
        "value" : "${access_key}"
      },
      {
        "name" : "AWS_SECRET_ACCESS_KEY",
        "value" : "${secret_key}"
      },
      {
        "name" : "AWS_REGION",
        "value" : "${region}"
      },
      {
        "name" : "VERSION",
        "value" : "0.9.1"
      }
    ]
  }
]
