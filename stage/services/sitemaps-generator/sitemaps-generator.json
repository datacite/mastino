[
  {
    "name": "sitemaps-generator-test",
    "image": "datacite/maltese",
    "cpu": 256,
    "memory": 1000,
    "essential": true,
    "environment" : [
      {
        "name" : "SITEMAP_BUCKET",
        "value" : "search.test.datacite.org"
      },
      {
        "name" : "RACK_ENV",
        "value" : "stage"
      },
      {
        "name" : "AWS_ACCESS_KEY_ID",
        "value" : "${access_key}"
      },
      {
        "name" : "AWS_SECRET_ACCESS_KEY",
        "value" : "${secret_key}"
      },
      {
        "name" : "AWS_REGION",
        "value" : "${region}"
      },
      {
        "name" : "REVISION",
        "value" : "8"
      }
    ]
  }
]
