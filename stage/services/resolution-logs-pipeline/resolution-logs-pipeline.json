[
  {
    "name": "resolution-logs-pipeline-stage",
    "image": "datacite/shiba-inu",
    "cpu": 512,
    "memory": 2048,
    "essential": true,
    "networkMode": "awsvpc",
    "hostname": "resolution-logs-pipeline-stage",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
            }
    ],
    "logConfiguration": { 
      "logDriver": "awslogs",
      "options": { 
         "awslogs-group" : "/ecs/resolution-logs-pipeline-stage",
         "awslogs-region": "eu-west-1",
         "awslogs-stream-prefix": "ecs"
      }
    },
    "environment" : [
      {
        "name" : "LOG_LEVEL",
        "value" : "debug"
      },
      {
        "name" : "JWT_PUBLIC_KEY",
        "value" : "${jwt_public_key}"
      },
      {
        "name" : "JWT_PRIVATE_KEY",
        "value" : "${jwt_private_key}"
      },
      {
        "name" : "BUGSNAG_KEY",
        "value" : "${bugsnag_key}"
      },
      {
        "name" : "PUBLIC_KEY",
        "value" : "${public_key}"
      },
      {
        "name" : "AWS_REGION",
        "value" : "${region}"
      },
      {
        "name" : "AWS_ACCESS_KEY_ID",
        "value" : "${access_key}"
      },
            {
        "name" : "AWS_SECRET_ACCESS_KEY",
        "value" : "${secret_key}"
      },
      {
        "name" : "S3_RESOLUTION_LOGS_BUCKET",
        "value" : "${s3_resolution_logs_bucket}"
      },
      {
        "name" : "S3_MERGED_LOGS_BUCKET",
        "value" : "${s3_merged_logs_bucket}"
      },
      {
        "name" : "ES_HOST",
        "value" : "${es_host}"
      },
      {
        "name" : "ES_NAME",
        "value" : "${es_name}"
      },
      {
        "name" : "ES_INDEX",
        "value" : "${es_index}"
      },
      {
        "name" : "GITHUB_VERSION",
        "value" : "${version}"
      }
    ]
  }
]
