[
  {
    "name": "usage-update-stage",
    "image": "datacite/toccatore",
    "cpu": 256,
    "memory": 1000,
    "essential": true,
    "command": [
      "toccatore", "usage-update",
      "--push_url", "${push_url}",
      "--access_token", "${access_token}",
      "--source_token", "${source_token}"
    ],
    "environment" : [
      {
        "name" : "ENVIRONMENT",
        "value" : "${environment}"
      },
      {
        "name" : "AWS_REGION",
        "value" : "${region}"
      }
    ]
  }
]
