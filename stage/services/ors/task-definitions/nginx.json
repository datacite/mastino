[
   {
      "name": "nginx",
      "image": "jackofsum/ors-nginx:latest",
      "essential": true,
      "portMappings" : [
         {
            "containerPort": 80,
            "hostPort": 80
         }
      ],
      "logConfiguration": { 
            "logDriver": "awslogs",
            "options": { 
                  "awslogs-group" : "/ecs/nginx-stage",
                  "awslogs-region": "eu-west-1",
                  "awslogs-stream-prefix": "ecs"
            }
      }
   }
]
