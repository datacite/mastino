[
  {
    "name": "cheetoh-test",
    "image": "datacite/cheetoh",
    "cpu": 512,
    "memory": 1024,
    "essential": true,
    "networkMode": "awsvpc",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "logConfiguration": { 
      "logDriver": "awslogs",
      "options": { 
         "awslogs-group" : "/ecs/cheetoh-test",
         "awslogs-region": "eu-west-1",
         "awslogs-stream-prefix": "ecs"
      }
    },
    "environment" : [
      {
        "name" : "PASSENGER_APP_ENV",
        "value" : "test"
      },
      {
        "name" : "RAILS_ENV",
        "value" : "test"
      },
      {
        "name" : "SITE_TITLE",
        "value" : "DataCite EZ API Test"
      },
      {
        "name" : "API_URL",
        "value" : "${api_url}"
      },
      {
        "name" : "LOG_LEVEL",
        "value" : "warn"
      },
      {
        "name" : "SECRET_KEY_BASE",
        "value" : "${secret_key_base}"
      },
      {
        "name" : "SENTRY_DSN",
        "value" : "${sentry_dsn}"
      },
      {
        "name" : "ADMIN_USERNAME",
        "value" : "${admin_username}"
      },
      {
        "name" : "ADMIN_PASSWORD",
        "value" : "${admin_password}"
      },
      {
        "name" : "SERVERNAME",
        "value" : "cheetoh.test.datacite.org"
      },
      {
        "name" : "MEMCACHE_SERVERS",
        "value" : "${memcache_servers}"
      },
      {
        "name" : "SANDBOX",
        "value" : "1"
      },
      {
        "name" : "GITHUB_VERSION",
        "value" : "${version}"
      }
    ]
  }
]
